{
  "name": "linter-ktlint",
  "main": "./lib/linter-ktlint.js",
  "version": "0.0.0",
  "private": true,
  "description": "Lint Kotlin files using ktlint",
  "keywords": [
    "lint",
    "ktlint",
    "kotlin"
  ],
  "repository": "https://github.com/kkopper/atom-linter-ktlint",
  "license": "MIT",
  "configSchema": {
    "executablePath": {
      "type": "string",
      "default": "ktlint",
      "title": "Executable path",
      "description": "Please specify where your `ktlint` executable is located."
    },
    "verbose": {
      "type": "boolean",
      "default": false,
      "title": "Verbose mode",
      "description": "Run ktlint in verbose mode."
    },
    "experimental": {
      "type": "boolean",
      "default": false,
      "title": "Enable experimental rulesets",
      "description": "Run ktlint with experimental rulesets enabled."
    }
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "10.0.0",
    "atom-package-deps": "5.1.0"
  },
  "devDependencies": {
    "jasmine-fix": "1.3.1"
  },
  "package-deps": [
    "linter:2.0.0",
    "language-kotlin"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  }
}
